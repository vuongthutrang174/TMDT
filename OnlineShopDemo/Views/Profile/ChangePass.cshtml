
@{
    ViewData["Title"] = "ChangePass";
    Layout = "~/Views/Shared/_Layout.cshtml";
}



@model OnlineShopDemo.Models.UserModel.Client
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers

<div class="change-pass">
    <div class="model-content">
        <form asp-action="ChangePass" asp-controller="Profile" method="post">
            <img src="/img/core-img/logo.png" class="pop-up-logo" alt="">
            <p>Your new Password is not Match</p>
            <p>Or your old password verify is incorrect</p>
            <a onclick="location.href='@Url.Action("Index","Profile")'" class="btn amado-btn w-100 active pass-change ">Back</a>
        </form>
    </div>
</div>

